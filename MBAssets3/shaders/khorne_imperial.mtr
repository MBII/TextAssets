textures/khorne_imperial/textured_glass_cyan
{
    qer_editorimage  textures/khorne_imperial/textured_glass_cyan
    q3map_material    Glass
    qer_trans    0.5
    surfaceparm    trans
    surfaceparm    nonopaque
    surfaceparm    lightfilter
    // Dummy stage to force solid sort -> working pbr materials...
    {
        map $whiteimage
        alphaGen const 0.0
        alphaFunc GE128
        depthWrite
    }
    // Filter stage
    {
        map textures/khorne_pbr/textured_glass_cyan_a //This controls color.
        blendFunc GL_DST_COLOR GL_ZERO
    }
    // Specular stage
    {
        map $whiteimage
        normalMap textures/khorne_pbr/textured_glass_cyan_nh
        rgbGen const ( 0 0 0 )            // No diffuse contribution from this stage
        specularMap $whiteimage            // 100% reflectivity
        gloss 0.98                        // Make it not perfectly glossy
        alphaGen const 0.40               // Reduce alpha and therefore reflectivity to 4%
        blendFunc GL_SRC_ALPHA GL_ONE    // Just add the specular
    }
    {
        map $lightmap
        blendFunc GL_DST_COLOR GL_ZERO
    }
}
