textures/khorne_imperial/floor_grate_1
{
	qer_editorimage	textures/khorne_imp_q3/floor_grate_1
	surfaceparm	nonopaque
	q3map_alphashadow
	cull twosided
    {
        map textures/khorne_pbr/floor_grate_1_a
		normalHeightMap textures/khorne_pbr/floor_grate_1_nh
		rmoMap textures/khorne_pbr/floor_grate_1_rmo
		parallaxDepth 0.1 parallaxBias 0.55
        alphaFunc GE128
        blendFunc GL_ONE GL_ZERO
        depthWrite
        rgbGen identity
    }
    {
        map $lightmap
        alphaFunc GT0
        blendFunc GL_DST_COLOR GL_ZERO
    }
}

