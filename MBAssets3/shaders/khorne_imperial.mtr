textures/khorne_imperial/landing
{
    qer_editorimage    textures/khorne_imperial/landing
    q3map_material    SolidMetal
    {
        map $lightmap
    }
    {    
        map textures/khorne_pbr/landing_a
        normalHeightMap textures/khorne_pbr/landing_nh
        rmoMap textures/khorne_pbr/landing_rmo
        blendfunc GL_DST_COLOR GL_ZERO 
        parallaxDepth 0.019 parallaxBias 0.55
    }
    {
        oneshotanimMap 1 textures/khorne_imperial/landing_glow textures/colors/black
        blendFunc GL_ONE GL_ONE
        glow
                rgbGen const ( 0.900000 0.2500000 0.000000 )
    }
    {
        oneshotanimMap 1 textures/colors/black textures/khorne_imperial/landing_glow
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen const ( 0.000000 1.000000 1.000000 )
    }
}

