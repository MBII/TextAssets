models/players/Shoretrooper/head
{
	q3map_nolightmap
	{
		map models/players/Shoretrooper/head
		rgbGen lightingDiffuse
	}
	{
		map models/players/Shoretrooper/head_specular
		blendFunc GL_SRC_ALPHA GL_ONE
		detail
		alphaGen lightingSpecular
	}
}

