textures/amace_emperor/mp_win02
{
	qer_editorimage	textures/amace_emperor/mp_win02
	surfaceparm nonopaque
	surfaceparm trans
	surfaceparm alphashadow 
	qer_trans 1.0 
    {
        map textures/amace_emperor/mp_win02
		alphaFunc GE128
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		depthWrite
    }
    {
        map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
		depthFunc equal
    }
}

