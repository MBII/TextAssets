textures/amace_lunarbase/cameraportal
{
	qer_editorimage	textures/system/qer_portal
	surfaceparm	nonsolid
	surfaceparm	playerclip
	surfaceparm nomarks
	portal
	q3map_nolightmap
	{
		map gfx/effects/decoystatic2
		blendFunc GL_ONE GL_ONE
		rgbGen const ( 0.25 0.25 0.25 )
		tcMod scroll 5 2
	}
	{
		map gfx/effects/decoystatic2
		blendFunc GL_ONE GL_ONE
		rgbGen const ( 0.25 0.25 0.25 )
		tcMod scroll -2 -2
    }
    {
		map textures/colors/black
		blendFunc GL_ONE GL_ONE_MINUS_SRC_ALPHA
		depthWrite
		alphaGen portal 424
    }
}

textures/amace_lunarbase/brokencamera
{
	qer_editorimage	gfx/2d/brokencamera
	surfaceparm	nomarks
	surfaceparm	nonsolid
	surfaceparm	playerclip
	q3map_nolightmap
    {
		map textures/colors/black
		blendFunc GL_ONE GL_ONE_MINUS_SRC_ALPHA
    }
    {
		map gfx/2d/brokencamera
		blendFunc GL_ONE GL_ONE
		rgbGen identity
		tcMod scroll 5 2
    }
    {
		map gfx/2d/brokencamera
		blendFunc GL_ONE_MINUS_DST_COLOR GL_ONE_MINUS_SRC_COLOR
		rgbGen identity
		tcMod scroll -2 -2
    }
}

