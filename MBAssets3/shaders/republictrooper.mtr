// Default

models/players/republictrooper/helmet
{
	{
		map models/players/republictrooper/helmet_A
		normalMap models/players/republictrooper/helmet_N
		rmoMap models/players/republictrooper/helmet_RMO
		rgbGen lightingDiffuse
	}
}

