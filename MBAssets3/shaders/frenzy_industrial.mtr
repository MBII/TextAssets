textures/frenzy_industrial/frenzdoor1
{
	{
		map $lightmap
	}
	{
		map textures/frenzy_industrial_pbr/frenzdoor1_a
		normalHeightMap textures/frenzy_industrial_pbr/frenzdoor1_nh
		rmoMap textures/frenzy_industrial_pbr/frenzdoor1_rmo
		blendfunc GL_DST_COLOR GL_ZERO 
		parallaxDepth 0.019 parallaxBias 0.55
	}
    {
        map textures/frenzy_industrial/frenzdoor1_glow_green
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen const ( 0.000000 1.000000 0.000000 )
    }
}

textures/frenzy_industrial/frenzdoor1_lockable
{
    qer_editorimage    textures/frenzy_industrial/frenzdoor1
    {
        map $lightmap
    }
    {
        map textures/frenzy_industrial_pbr/frenzdoor1_a
        normalHeightMap textures/frenzy_industrial_pbr/frenzdoor1_nh
        rmoMap textures/frenzy_industrial_pbr/frenzdoor1_rmo
        blendfunc GL_DST_COLOR GL_ZERO 
        parallaxDepth 0.019 parallaxBias 0.55
    }
    {
        oneshotanimMap 1 textures/frenzy_industrial/frenzdoor1_glow_white textures/colors/black
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen const ( 1.000000 0.000000 0.000000 )
    }
    
    {
        oneshotanimMap 1 textures/colors/black textures/frenzy_industrial/frenzdoor1_glow_white
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen const ( 0.000000 1.000000 0.000000 )
    }
}

