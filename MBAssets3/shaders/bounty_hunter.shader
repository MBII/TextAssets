models/players/bounty_hunter/bounty_hunter_helmet
{
    {
        map models/players/bounty_hunter/bounty_hunter_helmet
        rgbGen lightingDiffuse
    }
    {
        map models/players/bounty_hunter/bounty_hunter_helmet_glow
        blendFunc GL_ONE GL_ONE
        rgbGen identity
    }
    {
        map models/players/bounty_hunter/bounty_hunter_helmet_s
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

