models/weapons2/w_jawa/w_jawa
{
	q3map_nolightmap
    {
        map models/weapons2/w_jawa/w_jawa
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/weapons2/w_jawa/w_jawa_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
