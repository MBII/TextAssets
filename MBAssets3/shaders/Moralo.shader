models/players/Moralo/chest
{
    {
        map models/players/Moralo/chest
        rgbGen lightingDiffuse
    }
    {
        map models/players/Moralo/chest_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
	detail
    }
    {
        map models/players/Moralo/chest_glow
        blendFunc GL_ONE GL_ONE
        detail
        rgbGen identity
    }
}


