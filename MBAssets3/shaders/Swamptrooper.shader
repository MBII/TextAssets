// Swamp Trooper MB2

models/players/swamptrooper/body
{
	q3map_nolightmap
	{
		map models/players/swamptrooper/body
		rgbGen lightingDiffuse
	}
}
models/players/swamptrooper/body_green
{
	q3map_nolightmap
	{
		map models/players/swamptrooper/body_green
		rgbGen lightingDiffuse
	}
}
