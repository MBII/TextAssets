
// Sith Officer

models/players/sith_officer/hipsOfficer
{
	q3map_nolightmap
    {
		map models/players/sith_officer/hipsOfficer
		rgbGen lightingDiffuse
	}
	{
		map models/players/hux/hips_specular
		blendFunc GL_SRC_ALPHA GL_ONE
		detail
		alphaGen lightingSpecular
	}
}

