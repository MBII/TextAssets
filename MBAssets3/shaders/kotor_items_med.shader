models/items/kotor/a_kit
{
    {
        map models/items/kotor/a_kit
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/items/kotor/a_kit_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
    {
        map models/items/kotor/a_kit_glow
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen identity
    }
}

