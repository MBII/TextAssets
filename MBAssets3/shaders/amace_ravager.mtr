//////////// MB2 Automap ////////////
gfx/mplevels/mb2_ravager/ravagermap
{
	nopicmip
    {
		map gfx/mplevels/mb2_ravager/ravagermap
		blendfunc blend
		rgbGen vertex
    }
}

//////////// Light and Glow Shaders ////////////
textures/amace_ravager/basemetal3red
{
	qer_editorimage	textures/amace_ravager/basemetal3red
	q3map_surfacelight	1400
	q3map_lightsubdivide	512
    {
        map $lightmap
    }
    {
        map textures/amace_ravager/basemetal3red
		normalMap textures/amace_ravager/basemetal3red_N
		rmoMap textures/amace_ravager/basemetal3red_RMO
        blendFunc GL_DST_COLOR GL_ZERO
    }
    {
        map textures/amace_ravager/basemetal3red_glow
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen identity
    }
}

textures/amace_ravager/console2
