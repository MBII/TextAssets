////////// Reflective Shaders /////////
textures/amace_leviathan/pipes1
{
	qer_editorimage	textures/amace_leviathan/pipes1
	q3map_material	SolidMetal
    {
        map $lightmap
    }
    {
        map textures/amace_leviathan/pipes1
        blendFunc GL_DST_COLOR GL_ZERO
    }
    {
        map textures/common/etest4
        blendFunc GL_DST_COLOR GL_ONE
        tcGen environment
	detail
    }
}

////////// Skies /////////
textures/amace_ravager/ravager
{
	qer_editorimage	textures/skies/sky
	surfaceparm	sky
	surfaceparm	noimpact
	surfaceparm	nomarks
	surfaceparm	nodlight
	nopicmip
	nomipmaps
	q3map_nolightmap
	skyParms	textures/amace_ravager/ravager 1024 -
}

////////// Fog /////////
textures/amace_ravager/whitefog
{
	qer_editorimage	textures/fogs/fog
	qer_nocarve
	qer_trans	0.5
	surfaceparm	nonsolid
	surfaceparm	nonopaque
	surfaceparm	fog
	surfaceparm	trans
	q3map_nolightmap
	fogparms	( 1 1 1 ) 13304.0
}

