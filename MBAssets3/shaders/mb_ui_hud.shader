gfx/hud/alpha/HUD_CaptureProgress
{
	nopicmip
	notc
	{
		map gfx/hud/alpha/HUD_CaptureBar
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen vertex
	}
	{
		map gfx/hud/alpha/HUD_CaptureProgress
		blendFunc GL_ONE GL_ONE
		rgbGen const ( 0.5 0.5 0.5 )
		tcmod scroll -0.35 0
	}
	{
		map gfx/hud/alpha/HUD_CaptureBar_Mask
		blendFunc GL_ZERO GL_SRC_COLOR
	}
}

gfx/hud/alpha/HUD_CaptureRegress
{
	nopicmip
	notc
	{
		map gfx/hud/alpha/HUD_CaptureBar
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen vertex
	}
	{
		map gfx/hud/alpha/HUD_CaptureProgress
		blendFunc GL_ONE GL_ONE
		rgbGen const ( 0.5 0.5 0.5 )
		tcmod scale	-1 1
		tcmod scroll -0.35 0
	}
	{
		map gfx/hud/alpha/HUD_CaptureBar_Mask
		blendFunc GL_ZERO GL_SRC_COLOR
	}
}

