models/players/Krennic/torso
{
	q3map_nolightmap
	{
		map models/players/Krennic/torso
		rgbGen lightingDiffuse
	}
	{
		map models/players/Krennic/torso_specular
		blendFunc GL_SRC_ALPHA GL_ONE
		detail
		alphaGen lightingSpecular
	}
}

