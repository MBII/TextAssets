// Kaadu animal shader
//twosided+spec
models/players/kaadu/straps
{
	cull	twosided
    {
        map models/players/kaadu/straps
        rgbGen lightingDiffuse
    }
    {
        map models/players/kaadu/straps_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
		detail
    }
}

models/players/kaadu/straps_sec
{
	cull	twosided
    {
        map models/players/kaadu/straps_sec
        rgbGen lightingDiffuse
