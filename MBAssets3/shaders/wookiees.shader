// Chewbacca C-3PO

models/players/chewbacca/c3po_face
{
	{
		map models/players/chewbacca/c3po_face
		blendFunc GL_ONE GL_ZERO
		rgbGen lightingDiffuse
	}
	{
		map models/players/c3po/c3po_face_glow
		blendFunc GL_ONE GL_ONE
		rgbGen identity
	}
	{
		map models/players/c3po/env
		blendFunc GL_DST_COLOR GL_SRC_COLOR
		detail
		tcGen environment
	}
	{
		map models/players/c3po/c3po_face_s
		blendFunc GL_SRC_ALPHA GL_ONE
		detail
		alphaGen lightingSpecular
	}
}

models/players/chewbacca/c3po_arm_torso
{
	{
		map models/players/chewbacca/c3po_arm_torso
