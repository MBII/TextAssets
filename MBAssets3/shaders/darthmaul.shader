models/players/DarthMaul/body
{
	{
		map models/players/DarthMaul/body
		rgbGen lightingDiffuse
	}
	{
		map models/players/DarthMaul/body_s
		blendFunc GL_SRC_ALPHA GL_ONE
		alphaGen lightingSpecular
